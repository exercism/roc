{%- import "generator_macros.j2" as macros with context -%}
{{ macros.canonical_ref() }}
{{ macros.header() }}

import {{ exercise | to_pascal }} exposing [{{ cases[0]["property"] | to_snake }}]

{% for case in cases -%}
# {{ case["description"] }}
expect
    garden = {{ case["input"]["garden"] | to_roc_multiline_string | replace(" ", "路") | indent(8) }} |> Str.replace_each "路" " "
    result = {{ case["property"] | to_snake }} garden
    expected = {{ case["expected"] | to_roc_multiline_string | replace(" ", "路") | indent(8) }}  |> Str.replace_each "路" " "
    result == expected

{% endfor %}
